package com;

public class DemoTest {

	public static void main(String[] args) {
	Employee emp = new Employee();
	emp.setId(100);
	emp.setName("Ravi");
	emp.setSalary(14000);
	
	System.out.println("id is "+emp.getId());
	System.out.println("name is "+emp.getName());
	System.out.println("salary is "+emp.getSalary());
	}

}
